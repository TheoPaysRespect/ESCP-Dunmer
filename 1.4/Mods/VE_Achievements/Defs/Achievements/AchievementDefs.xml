<Defs>
	<!-- ==========[ESCP_AchievementDunmer_BuildAshBurialPits]========== -->
	<AchievementsExpanded.AchievementDef ParentName="ESCP_Achievement_Base">
		<defName>ESCP_AchievementDunmer_BuildAshBurialPits</defName>
		<label>Not For Playing In</label>
		<description>Build 10 Ash Burial Pit segments</description>
		<tracker Class="Mashed_VanillaAchievementsExpanded.BuildingTracker_WithDefList">
			<defList>
				<li>ESCP_Dunmer_AshBurialPit_SquareCorner</li>
				<li>ESCP_Dunmer_AshBurialPit_RoundedCorner</li>
			</defList>
			<count>10</count>
		</tracker>
		<texPath>Achievements/ESCP_AchievementDunmer_BuildAshBurialPits</texPath>
		<points>5</points>
	</AchievementsExpanded.AchievementDef>
