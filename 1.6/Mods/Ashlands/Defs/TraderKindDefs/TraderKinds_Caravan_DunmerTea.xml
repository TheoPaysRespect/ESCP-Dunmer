<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <TraderKindDef>
        <defName>ESCP_DunmerCaravan_TeaTrader</defName>
        <label>Dunmer tea trader</label>
        <stockGenerators>
            <!-- Resources -->
            <li Class="StockGenerator_SingleDef">
                <thingDef>Silver</thingDef>
                <countRange>900~1500</countRange>
            </li>
            <li Class="StockGenerator_SingleDef">
                <thingDef>Mashed_Ashlands_Scuttle</thingDef>
                <countRange>20~30</countRange>
            </li>
            <li Class="StockGenerator_Tag">
                <tradeTag>Mashed_Ashlands_AshlandsRawTeaPlant</tradeTag>
                <thingDefCountRange>1~3</thingDefCountRange>
                <totalPriceRange>100~300</totalPriceRange>
            </li>
            <!-- Drugs -->
            <li Class="StockGenerator_Tag">
                <tradeTag>Mashed_Ashlands_AshlandsTea</tradeTag>
                <thingDefCountRange>2~4</thingDefCountRange>
                <totalPriceRange>300~800</totalPriceRange>
            </li>
            <!-- Techprints -->
            <li Class="StockGenerator_Techprints">
                <countChances>
                    <li>
                        <count>1</count>
                        <chance>1</chance>
                    </li>
                </countChances>
            </li>
            <!-- Buying -->
            <li Class="StockGenerator_BuySlaves"/>
            <li Class="StockGenerator_BuyExpensiveSimple"/>
            <li Class="StockGenerator_BuyTradeTag">
                <tag>Art</tag>
            </li>
            <li Class="StockGenerator_BuyTradeTag">
                <tag>MusicalInstrument</tag>
            </li>
        </stockGenerators>
    </TraderKindDef>
</Defs>
