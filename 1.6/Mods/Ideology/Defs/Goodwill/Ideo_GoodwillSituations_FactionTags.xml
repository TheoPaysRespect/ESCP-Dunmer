	</GoodwillSituationDef>
	<!-- ==========[ESCP_Ashlander_GreatHouse]========== -->
	<GoodwillSituationDef ParentName="ESCP_DunmerGoodwillSituation_Base">
		<defName>ESCP_Ashlander_GreatHouse</defName>
		<modExtensions>
			<li Class="ESCP_RaceTools.FactionGoodwillProperties">
				<FactionTagA>ESCP_DunmerAshlander</FactionTagA>
